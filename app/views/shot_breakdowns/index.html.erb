<p id="notice"><%= notice %></p>

<h1>Listing Shot Breakdowns</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shot_breakdowns.each do |shot_breakdown| %>
      <tr>
        <td><%= link_to 'Show', shot_breakdown %></td>
        <td><%= link_to 'Edit', edit_shot_breakdown_path(shot_breakdown) %></td>
        <td><%= link_to 'Destroy', shot_breakdown, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shot breakdown', new_shot_breakdown_path %>
