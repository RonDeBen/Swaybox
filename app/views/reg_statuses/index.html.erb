<p id="notice"><%= notice %></p>

<h1>Listing Reg Statuses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reg_statuses.each do |reg_status| %>
      <tr>
        <td><%= link_to 'Show', reg_status %></td>
        <td><%= link_to 'Edit', edit_reg_status_path(reg_status) %></td>
        <td><%= link_to 'Destroy', reg_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reg status', new_reg_status_path %>
