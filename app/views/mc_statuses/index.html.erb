<p id="notice"><%= notice %></p>

<h1>Listing Mc Statuses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mc_statuses.each do |mc_status| %>
      <tr>
        <td><%= link_to 'Show', mc_status %></td>
        <td><%= link_to 'Edit', edit_mc_status_path(mc_status) %></td>
        <td><%= link_to 'Destroy', mc_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mc status', new_mc_status_path %>
